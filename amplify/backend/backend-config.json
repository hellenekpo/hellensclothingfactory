{
  "api": {
    "hcfbackend": {
      "dependsOn": [
        {
          "attributes": [
            "Name",
            "Arn"
          ],
          "category": "function",
          "resourceName": "hellensclothingfactory"
        }
      ],
      "providerPlugin": "awscloudformation",
      "service": "API Gateway"
